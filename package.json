{
  "name": "slpdex",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "analyze": "ng build --aot --prod --build-optimizer --stats-json && webpack-bundle-analyzer dist/slpdex/stats-es2015.json",
    "build": "ng build",
    "dev": "ng serve",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "postinstall": "node clean-moment-locales.js",
    "prod": "node clean-prod.js && ng build --aot --prod --build-optimizer --output-path=prod/ --delete-output-path=false && node copy-index.js",
    "push-prod": "cd prod && git add . && git commit -m \"build(slpdev): release\" && git push",
    "release": "npm run prod && npm run push-prod",
    "start": "ng serve",
    "svg": "cd src/svgs/ && node generate-svg-sprite.js && cd ../../ && node copy-svgs.js",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "~8.1.0-next.2",
    "@angular/cdk": "~8.0.1",
    "@angular/common": "~8.1.0-next.2",
    "@angular/compiler": "~8.1.0-next.2",
    "@angular/core": "~8.1.0-next.2",
    "@angular/forms": "~8.1.0-next.2",
    "@angular/material": "~8.0.1",
    "@angular/platform-browser": "~8.1.0-next.2",
    "@angular/platform-browser-dynamic": "~8.1.0-next.2",
    "@angular/pwa": "^0.800.3",
    "@angular/router": "~8.1.0-next.2",
    "@angular/service-worker": "~8.1.0-next.2",
    "bignumber.js": "^9.0.0",
    "cashcontracts": "^0.1.25",
    "core-js": "2.6.8",
    "countup.js-angular2": "^7.3.0",
    "jdenticon": "^2.1.1",
    "js-base64": "^2.5.1",
    "moment": "^2.24.0",
    "noty": "^3.2.0-beta",
    "qrcode": "^1.3.3",
    "rxjs": "~6.5.2",
    "simplebar": "^4.0.0",
    "slpdex-market": "^0.1.14",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.3",
    "@angular/cli": "~8.0.3",
    "@angular/compiler-cli": "~8.1.0-next.2",
    "@angular/language-service": "~8.1.0-next.2",
    "@types/jasmine": "~3.3.13",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.0.3",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.8.0",
    "svg-sprite": "^1.5.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.17.0",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
